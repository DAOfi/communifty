  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.2",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@openzeppelin/contracts": "4.2.0",
    "@types/chai": "4.2.21",
    "@types/express": "4.17.13",
    "@types/mocha": "9.0.0",
    "@types/node": "16.6.1",
    "axios": "0.21.1",
    "chai": "4.3.4",
    "ethereum-waffle": "3.4.0",
    "ethereumjs-util": "7.1.0",
    "ethers": "5.4.4",
    "hardhat": "2.6.0",
    "mocha": "9.0.3",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  },
  "scripts": {
    "lint": "yarn prettier ./test/*.ts ./scripts/*.ts ./src/*.ts --check",
